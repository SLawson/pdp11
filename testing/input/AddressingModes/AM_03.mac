; File: AM_03.mac
; Tests: Auto Increment addressing

.RADIX 10
.PSECT ABS

.=.+100
        .WORD 20
        .WORD 30

.=.+200
        .WORD 90
        .WORD 80

.PSECT REL

START:
        MOV   #100, R0
        MOV   #200, R1
        MOV   #202, R2
        MOV   (R0)+, (R1)+
        MOV   (R0)+, (R1)+
        MOV   @R2, R3
        CCC
        HALT

